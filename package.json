{
  "name": "my-starter-kit-dashboard",
  "version": "0.1.0",
  "private": false,
  "homepage": "http://gonghaima.github.io/claims",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cypress": "^3.3.1",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "prop-types": "15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^5.0.1",
    "react-spring": "^8.0.23",
    "sass": "^1.55.0",
    "source-map-explorer": "^1.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test-coverage": "yarn test --coverage --watchAll=false",
    "snap-shot-update": "yarn test -u",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "precommit": "yarn test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "cypress run --record --key 7e57d88b-9be7-44cf-8039-47263ff266f3",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.{js,jsx,ts,tsx}.snap",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/config/index.js",
      "!<rootDir>/src/icons/index.js",
      "!<rootDir>/src/icons/BurstMode.js",
      "!<rootDir>/src/icons/Person.js",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
